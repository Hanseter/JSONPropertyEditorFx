{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "object": {
      "type": "object",
      "default": {"a":  "Hello World"},
      "properties": {
        "a": {
          "type": "string"
        }
      }
    },
    "array": {
      "type": "array",
      "items": {
        "type": "object",
        "default": {},
        "properties": {
          "a": {
            "type": "string"
          }
        }
      },
      "default": [{"a":  "Hello World"},{"a":  "Good Bye World"}]
    }
  }
}
